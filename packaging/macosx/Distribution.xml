<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-gui-script minSpecVersion="1">
    <pkg-ref id="org.TBI.ViennaRNA.core"/>
    <pkg-ref id="org.TBI.ViennaRNA.cluster"/>
    <pkg-ref id="org.TBI.ViennaRNA.kinfold"/>
    <pkg-ref id="org.TBI.ViennaRNA.rnaforester"/>
    <pkg-ref id="org.TBI.ViennaRNA.kinwalker"/>
    <pkg-ref id="org.TBI.ViennaRNA.rnalocmin"/>
    <pkg-ref id="org.TBI.ViennaRNA.libRNA"/>
    <pkg-ref id="org.TBI.ViennaRNA.tutorial"/>
    <pkg-ref id="org.TBI.ViennaRNA.perl"/>
    <pkg-ref id="org.TBI.ViennaRNA.python"/>
    <pkg-ref id="org.TBI.ViennaRNA.python3"/>
    <options customize="always" hostArchitectures="x86_64" />
    <domains enable_localSystem="true" enableCurrentUserHome="false" enable_anywhere="false" />
    <background file="ohm.png" mime-type="image/png" alignment="right" scaling="none" />
    <title>ViennaRNA Package 2.6.4</title>
    <welcome file="welcome.txt" mime-type="text/plain" />
    <readme file="README.md" mime-type="text/plain" />
    <license file="COPYING" mime-type="text/plain" />
    <conclusion file="conclusion.html" mime-type="text/html" />
     <choices-outline>
        <line choice="org.TBI.ViennaRNA.core"/>
        <line choice="org.TBI.ViennaRNA.cluster"/>
        <line choice="org.TBI.ViennaRNA.kinfold"/>
        <line choice="org.TBI.ViennaRNA.rnaforester"/>
        <line choice="org.TBI.ViennaRNA.kinwalker"/>
        <line choice="org.TBI.ViennaRNA.rnalocmin"/>
        <line choice="org.TBI.ViennaRNA.libRNA"/>
        <line choice="org.TBI.ViennaRNA.tutorial"/>
        <line choice="org.TBI.ViennaRNA.perl"/>
        <line choice="org.TBI.ViennaRNA.python"/>
        <line choice="org.TBI.ViennaRNA.python3"/>
    </choices-outline>
    <choice id="org.TBI.ViennaRNA.core" title="Core" enabled="false"
        description="Executable programs, utilities, and corresponding manual pages">
        <pkg-ref id="org.TBI.ViennaRNA.core">#ViennaRNA%20Package%202.6.4%20Core.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.tutorial" title="RNA Bioinformatics Tutorial"
        description="A tutorial on RNA Bioinformatics using the tools of the ViennaRNA Package">
        <pkg-ref id="org.TBI.ViennaRNA.tutorial">#ViennaRNA%20Package%202.6.4%20Tutorial.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.cluster" title="Cluster Analysis" start_selected="false"
        description="The programs AnalyseSeqs and AnalyseDists offer some cluster analysis tools (split decomposition, statistical geometry, neighbor joining, Ward's method) for sequences and distance data">
        <pkg-ref id="org.TBI.ViennaRNA.cluster">#ViennaRNA%20Package%202.6.4%20Cluster%20Analysis.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.kinfold" title="Kinfold"
        description="Simulate kinetic folding of RNA secondary structures">
        <pkg-ref id="org.TBI.ViennaRNA.kinfold">#ViennaRNA%20Package%202.6.4%20Kinfold.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.rnaforester" title="RNAforester"
        description="Compare RNA secondary structures via forest alignment">
        <pkg-ref id="org.TBI.ViennaRNA.rnaforester">#ViennaRNA%20Package%202.6.4%20RNAforester.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.kinwalker" title="Kinwalker" start_selected="false"
        description="Predict (co-transcriptional) RNA folding trajectories">
        <pkg-ref id="org.TBI.ViennaRNA.kinwalker">#ViennaRNA%20Package%202.6.4%20Kinwalker.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.rnalocmin" title="RNAlocmin"
        description="Calculate local minima from structures via gradient walks">
        <pkg-ref id="org.TBI.ViennaRNA.rnalocmin">#ViennaRNA%20Package%202.6.4%20RNAlocmin.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.libRNA" title="Development Files"
        description="C-library, header files, and reference documentation">
        <pkg-ref id="org.TBI.ViennaRNA.libRNA">#ViennaRNA%20Package%202.6.4%20Development.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.perl" title="Perl Interface"
        description="Perl bindings for the C-library and additional Perl packages">
        <pkg-ref id="org.TBI.ViennaRNA.perl">#ViennaRNA%20Package%202.6.4Perl.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.python" title="Python 2 Interface"
        description="Python 2 bindings for the C-library">
        <pkg-ref id="org.TBI.ViennaRNA.python">#ViennaRNA%20Package%202.6.4Python.pkg</pkg-ref>
    </choice>
    <choice id="org.TBI.ViennaRNA.python3" title="Python 3 Interface"
        description="Python 3 bindings for the C-library">
        <pkg-ref id="org.TBI.ViennaRNA.python3">#ViennaRNA%20Package%202.6.4Python3.pkg</pkg-ref>
    </choice>
    <pkg-ref id="org.TBI.ViennaRNA.core" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Core.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.tutorial" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Tutorial.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.cluster" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Cluster%20Analysis.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.kinfold" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Kinfold.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.rnaforester" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20RNAforester.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.kinwalker" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Kinwalker.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.rnalocmin" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20RNAlocmin.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.libRNA" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Development.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.perl" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Perl.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.python" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Python.pkg</pkg-ref>
    <pkg-ref id="org.TBI.ViennaRNA.python3" version="2.6.4" onConclusion="none">ViennaRNA%20Package%202.6.4%20Python3.pkg</pkg-ref>
</installer-gui-script>
