      <memberdef kind="function" id="group__plot__probabilities_1ga4150e7dc7b6a9960b858d64abcd72290" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>int</type>
        <definition>int vrna_plot_dp_PS_list</definition>
        <argsstring>(char *seq, int cp, char *filename, vrna_ep_t *pl, vrna_ep_t *mf, char *comment)</argsstring>
        <name>vrna_plot_dp_PS_list</name>
        <param>
          <type>char *</type>
          <declname>seq</declname>
        </param>
        <param>
          <type>int</type>
          <declname>cp</declname>
        </param>
